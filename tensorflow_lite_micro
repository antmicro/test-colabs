ExecuteCommand("include @script.resc")
CreateTerminalTester("sysbus.{{uart_name}}", timeout=15)
StartEmulation()

WaitForLineOnUart("x_value: .* y_value: .*", treatAsRegex=True)
WaitForLineOnUart("x_value: .* y_value: .*", treatAsRegex=True)
WaitForLineOnUart("x_value: .* y_value: .*", treatAsRegex=True)
WaitForLineOnUart("x_value: .* y_value: .*", treatAsRegex=True)

print(ExecuteCommand("sysbus.{{uart_name}} DumpHistoryBuffer"))

ResetEmulation()

